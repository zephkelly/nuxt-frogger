{
  "name": "nuxt-frogger",
  "version": "0.1.9",
  "description": "Logging and tracing module for Nuxt applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zephkelly/nuxt-frogger.git"
  },
  "bugs": {
    "url": "https://github.com/zephkelly/nuxt-frogger/issues"
  },
  "homepage": "https://zephkelly.github.io/nuxt-frogger",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.19.3",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "vitepress-plugin-group-icons": "^1.6.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.19.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "happy-dom": "^20.0.8",
    "nuxt": "^3.19.3",
    "nuxt-auth-utils": "0.5.20",
    "playwright-core": "^1.56.1",
    "typescript": "~5.8.3",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "keywords": [
    "nuxtjs",
    "nuxt",
    "logging",
    "tracing",
    "monitoring",
    "observability",
    "frogger",
    "nuxt-frogger"
  ],
  "author": "zephkelly"
}
