{
  "name": "nuxt-frogger",
  "version": "0.1.3",
  "description": "Logging and tracing module for Nuxt applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zephkelly/nuxt-frogger.git"
  },
  "bugs": {
    "url": "https://github.com/zephkelly/nuxt-frogger/issues"
  },
  "homepage": "https://zephkelly.github.io/nuxt-frogger",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.3",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "vitepress-plugin-group-icons": "^1.5.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint-config": "^1.3.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.3",
    "@nuxt/test-utils": "^3.18.0",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.3",
    "nuxt-auth-utils": "0.5.20",
    "typescript": "~5.8.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "keywords": [
    "nuxtjs",
    "nuxt",
    "logging",
    "tracing",
    "monitoring",
    "observability",
    "frogger",
    "nuxt-frogger"
  ],
  "author": "zephkelly"
}
